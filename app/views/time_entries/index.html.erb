<p id="notice"><%= notice %></p>

<h1>Time Entries</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Begin at</th>
      <th>End at</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |time_entry| %>
      <tr>
        <td><%= time_entry.project.name %></td>
        <td><%= time_entry.begin_at %></td>
        <td><%= time_entry.end_at %></td>
        <td><%= time_entry.notes %></td>
        <td><%= link_to 'Show', time_entry %></td>
        <td><%= link_to 'Edit', edit_time_entry_path(time_entry) %></td>
        <td><%= link_to 'Destroy', time_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Entry', new_time_entry_path %>
